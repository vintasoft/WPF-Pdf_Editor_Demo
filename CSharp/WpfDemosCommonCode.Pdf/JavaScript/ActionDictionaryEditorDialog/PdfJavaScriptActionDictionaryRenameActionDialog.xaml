<Window x:Class="WpfDemosCommonCode.Pdf.PdfJavaScriptActionDictionaryRenameActionDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="PdfJavaScriptActionDictionaryAddDialog" Height="126" Width="530">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        
        <Label x:Name="messageLabel" VerticalAlignment="Center" Grid.Row="0" />
        
        <TextBox x:Name="textBox" Grid.Row="1" VerticalAlignment="Center" Margin="3,0" />
        
        <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Name="okButton" Margin="3" Width="75" Height="23" IsDefault="True" Click="okButton_Click">OK</Button>
            <Button Name="cancelButton" Margin="3" Width="75" Height="23" IsCancel="True" Click="cancelButton_Click">Cancel</Button>
        </StackPanel>
    </Grid>
</Window>

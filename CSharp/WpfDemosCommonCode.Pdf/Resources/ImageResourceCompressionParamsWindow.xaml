<Window x:Class="WpfDemosCommonCode.Pdf.ImageResourceCompressionParamsWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Image Resource Compression" Height="128" Width="271" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,3,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="94*" />
            <ColumnDefinition Width="155*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <Label Grid.Column="0" Grid.Row="0" VerticalAlignment="Center">Compression</Label>
        <ComboBox Name="compressionComboBox" Grid.Column="1" Grid.Row="0" VerticalAlignment="Center" Height="23"></ComboBox>
        <Button Name="compressionParamsButton" Grid.Column="1" Grid.Row="1" Height="23" Click="compressionParamsButton_Click">Compression Params...</Button>
        <Button Name="okButton" Grid.Column="1" Grid.Row="2" Height="23" Width="75" HorizontalAlignment="Left" Click="okButton_Click">OK</Button>
    </Grid>
</Window>
